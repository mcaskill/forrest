{
    "name": "omniphx/forrest",
    "description": "Salesforce / Force.com REST API Client with support for Laravel integration",
    "license":"MIT",
    "keywords":["salesforce", "laravel", "rest", "force.com", "force"],
    "authors": [
        {
            "name": "Matthew Mitchener",
            "homepage": "http://mattmitchener.com",
            "email": "mattjmitchener@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "henrikbjorn/phpspec-code-coverage": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Omniphx\\Forrest\\": "src/Omniphx/Forrest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        },
        "laravel": {
            "providers": [
                "Omniphx\\Forrest\\Providers\\Laravel\\ForrestServiceProvider"
            ],
            "aliases": {
                "Forrest": "Omniphx\\Forrest\\Providers\\Laravel\\Facades\\Forrest"
            }
        }
    },
    "minimum-stability": "stable"
}
